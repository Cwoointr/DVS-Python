icCamera", 0    Combo3.AddItem "DCNoCamera", 0    Select Case CameraData.SpecificCamType    Case DCNoCamera        Combo3.ListIndex = 0    Case DCGenericCamera        Combo3.ListIndex = 1    Case DC40        Combo3.ListIndex = 2    Case DC50        Combo3.ListIndex = 3    Case DC120        Combo3.ListIndex = 4    End Select    Combo4.AddItem "DCBitRate230400", 0    Combo4.AddItem "DCBitRate115200", 0    Combo4.AddItem "DCBitRate57600", 0    Combo4.AddItem "DCBitRate38400", 0    Combo4.AddItem "DCBitRate19200", 0    Combo4.AddItem "DCBitRate9600", 0    Combo4.AddItem "DCBitRateAuto", 0    Select Case CameraData.BitRate    Case DCBitRateAuto        Combo4.ListIndex = 0    Case DCBitRate9600        Combo4.ListIndex = 1    Case DCBitRate19200        Combo4.ListIndex = 2    Case DCBitRate38400        Combo4.ListIndex = 3    Case DCBitRate57600        Combo4.ListIndex = 4    Case DCBitRate115200        Combo4.ListIndex = 5    Case DCBitRate230400        Combo4.ListIndex = 6    End SelectEnd SubPrivate Sub OK_Click()    Dim Ret As Integer        CameraData.PortNum = Combo2.ListIndex        Select Case Combo1.ListIndex    Case 0        CameraData.CamType = DCNoCamera    Case 1        CameraData.CamType = DCGenericCamera    Case 2        CameraData.CamType = DC40    Case 3        CameraData.CamType = DC50    Case 4        CameraData.CamType = DC120    End Select        Select Case Combo3.ListIndex    Case 0        CameraData.SpecificCamType = DCNoCamera    Case 1        CameraData.SpecificCamType = DCGenericCamera    Case 2        CameraData.SpecificCamType = DC40    Case 3        CameraData.SpecificCamType = DC50    Case 4        CameraData.SpecificCamType = DC120    End Select        Select Case Combo4.ListIndex    Case 0        CameraData.BitRate = DCBitRateAuto    Case 1        CameraData.BitRate = DCBitRate9600    Case 2        CameraData.BitRate = DCBitRate19200    Case 3        CameraData.BitRate = DCBitRate38400    Case 4        CameraData.BitRate = DCBitRate57600    Case 5        CameraData.BitRate = DCBitRate115200    Case 6        CameraData.BitRate = DCBitRate230400    End Select        Form1.StatusBar1.SimpleText = "Now Calling DCOpenCamera..."    Form1.Refresh        Ret = DCOpenCamera(CameraData)    Form1.StatusBar1.SimpleText = "DCOpenCamera:" & DCMakeErrorMessage(Ret)    Unload DCOpenCameraDlgEnd Sub