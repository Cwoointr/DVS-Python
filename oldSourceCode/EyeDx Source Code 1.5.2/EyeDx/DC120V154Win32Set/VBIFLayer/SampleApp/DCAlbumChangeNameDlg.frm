VERSION 4.00Begin VB.Form DCAlbumChangeNameDlg    Caption         =   "DCAlbumChangeName"   ClientHeight    =   1440   ClientLeft      =   1140   ClientTop       =   1515   ClientWidth     =   4905   Height          =   1845   Left            =   1080   LinkTopic       =   "DCAlbumChangeNameDlg"   ScaleHeight     =   1440   ScaleWidth      =   4905   Top             =   1170   Width           =   5025   Begin VB.CommandButton Cancel       Caption         =   "Cancel"      Height          =   375      Left            =   3480      TabIndex        =   2      Top             =   720      Width           =   1095   End   Begin VB.CommandButton OK       Caption         =   "OK"      Default         =   -1  'True      Height          =   375      Left            =   3480      TabIndex        =   1      Top             =   240      Width           =   1095   End   Begin VB.TextBox Text1       Height          =   270      Left            =   1320      TabIndex        =   0      Text            =   "Text1"      Top             =   240      Width           =   1815   End   Begin VB.Label Label2       Caption         =   "AlbumName :"      Height          =   255      Left            =   240      TabIndex        =   3      Top             =   240      Width           =   1095   EndEndAttribute VB_Name = "DCAlbumChangeNameDlg"Attribute VB_Creatable = FalseAttribute VB_Exposed = FalsePrivate Sub Cancel_Click()    Unload DCAlbumChangeNameDlgEnd SubPrivate Sub OK_Click()    Dim Ret As Integer    Dim Cnt As Integer    Dim TempStr As String    Dim AlbumName(DC120OneAlbumLength) As Byte    For Cnt = 0 To DC120OneAlbumLength - 1        AlbumName(Cnt) = &H0    Next Cnt        TempStr = Text1    For Cnt = 0 To Len(TempStr) - 1        AlbumName(Cnt) = Asc(Mid(TempStr, Cnt + 1, 1))    Next Cnt    AlbumName(DC120OneAlbumLength - 1) = &H0        CamMemType = Val(Form1.Text2.Text)    AlbumNum = Val(Form1.Text3.Text)        Form1.StatusBar1.SimpleText = "Now Calling DCAlbumChangeName..."    Form1.Refresh        Ret = DCAlbumChangeName(CameraData, CamMemType, AlbumNum, AlbumName(0))    Form1.StatusBar1.SimpleText = "DCAlbumChangeName:" & DCMakeErrorMessage(Ret)        Unload DCAlbumChangeNameDlgEnd Sub