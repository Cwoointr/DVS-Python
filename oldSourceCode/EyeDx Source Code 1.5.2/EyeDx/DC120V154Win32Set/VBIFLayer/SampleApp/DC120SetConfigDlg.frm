DefFlashMode = DC120SetConfigDlg.Text1(14).Text    Ret = DCSetConfig(CameraData, GenericConfig, FullDC120Config)    Form1.StatusBar1.SimpleText = "DCSetConfig:" & DCMakeErrorMessage(Ret)    Unload DC120SetConfigDlgEnd Sub