// This wont compile, but if you look at AboutBox.rsrc, you'll understand // Basicly there are three HOTSPOT areas that use the CInternet class:// 1 mailTo me// 2 http Apple// 3 http Metrowerks// STR# indicesenum { eSumWareWWWIdx = 1, eSumWareMailIdx, eAppleWWWIdx, eMetrowerksWWWIdx};voidCVRoomApp::SetupDialogStrings(){	Str63 url;	::GetIndString(url, 128, eSumWareMailIdx);	Str15 qtVersStr;	UQuicktimeVR::GetQuickTimeVersion(qtVersStr);	Str15 qtVRVersStr;	UQuicktimeVR::GetQuickTimeVRVersion(qtVRVersStr);	/// VersRec *VersRecPtr, **VersRecHndl; see  Types.h	TString<Str31>	appVersion;	{		CSaveResFile  appRes(mAppResFile);		VersRecHndl  versH = (VersRecHndl)::Get1Resource('vers', 1);		if (versH)			appVersion.Assign((*versH)->shortVersion);	}	::ParamText(url, qtVersStr, qtVRVersStr, appVersion);}// HOTSPOT items (3 not enabled)enum { eSumWareWWWItm = 3, eSumWareMailItm = 4, eAppleWWWItm = 8, eMetrowerksWWWItm = 9};// ---------------------------------------------------------------------------//		¥ ShowAboutBox// ---------------------------------------------------------------------------//	Display the About Box for the Application////	This implementation just puts up an Alert Box. Override if you wish//	to display a more elaborate About Box.#include "CInternet.h"voidCVRoomApp::ShowAboutBox(){	SetupDialogStrings();	UDesktop::Deactivate();		// Alert will swallow Deactivate event	short exitAlert = ::Alert(ALRT_About, nil);	UDesktop::Activate();	{		TString<Str63> url;		CInternet  internet; // default 'MOSS' Netscape		switch (exitAlert)		{			case eSumWareWWWItm:		::GetIndString(url, 128, eSumWareMailIdx);		break;			case eSumWareMailItm:		::GetIndString(url, 128, eSumWareMailIdx);				if (url.Length())		url.Insert("\pmailto:", 1);				break;			case eAppleWWWItm:			::GetIndString(url, 128, eAppleWWWIdx);				break;			case eMetrowerksWWWItm:	::GetIndString(url, 128, eMetrowerksWWWIdx);		break;		}		if (url.Length())			switch (exitAlert)			{				case eSumWareWWWItm:				case eSumWareMailItm:				case eAppleWWWItm:				case eMetrowerksWWWItm:					internet.GetURL(url);					break;			}	}}